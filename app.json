{
	"expo": {
		"name": "Paredes Verdes",
		"slug": "paredes-verdes-app",
		"scheme": "paredes-verdes",
		"version": "1.0.3",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"userInterfaceStyle": "automatic",
		"assetBundlePatterns": ["**/*"],
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"config": {
				"usesNonExemptEncryption": false
			},
			"splash": {
				"backgroundColor": "#ffffff",
				"image": "./assets/images/splash.png",
				"dark": {
					"image": "./assets/images/splash-dark.png",
					"backgroundColor": "#04151A"
				}
			}
		},
		"android": {
			"splash": {
				"backgroundColor": "#ffffff",
				"image": "./assets/images/splash.png",
				"dark": {
					"image": "./assets/images/splash-dark.png",
					"backgroundColor": "#04151A"
				},
				"imageWidth": 200
			},
			"package": "com.abrahamgalue.paredesverdesapp"
		},
		"experiments": {
			"typedRoutes": true
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/Inter.otf"]
				}
			]
		],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "90c378ab-072c-4471-9914-e780671624d0"
			}
		}
	}
}
